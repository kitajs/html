{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "../",
    "jsxFactory": null,
    "jsxFragmentFactory": null,
    "noUnusedLocals": false
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.d.ts", "node_modules/csstype"],
  "exclude": ["node_modules", "dist", "coverage", "benchmark"]
}
